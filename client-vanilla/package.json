{
  "name": "client",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run build && npx live-server ./ --port=3000",
    "dev": "npx concurrently npm:watch:* npm:serve",
    "watch:js": "npx rollup -c --watch",
    "watch:css": "npx sass --watch src/styles/main.scss:build/styles/main.css",
    "serve": "npx live-server ./ --port=3000 --no-browser",
    "build": "rm -rf build && npx rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Gourav Khator",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "concurrently": "^7.0.0",
    "live-server": "^1.2.1",
    "rollup": "^2.65.0",
    "sass": "^1.49.0"
  },
  "dependencies": {
    "ipfs-core": "^0.13.0"
  }
}
