{
  "name": "stran",
  "description": "A Video Calling P2P DApp for masses to integrate strangers again..",
  "config": {
    "hardhatConfigPath": "./blockchain/hardhat.config.js",
    "deployContractScript": "./blockchain/scripts/deploy_contract.js"
  },
  "scripts": {
    "hardhat": "npx hardhat --config $npm_package_config_hardhatConfigPath",
    "full-install": "npm i && cd client && npm i && cd ../server && npm i",
    "sc-accounts": "npm run hardhat -- accounts",
    "sc-compile": "npm run hardhat -- compile",
    "sc-test": "npm run hardhat -- test",
    "sc-localnet": "npm run hardhat -- node",
    "sc-deploy": "npm run sc-compile && npm run hardhat -- run --network localhost $npm_package_config_deployContractScript",
    "app-start": "cd client && npm start",
    "app-test": "cd client && npm test",
    "server-start": "cd server && npm start",
    "server-dev": "cd server && npm run dev"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.3.4",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.5.2",
    "hardhat": "^2.8.0"
  },
  "repository": {
    "url": "https://github.com/gouravkhator/stran/"
  },
  "bugs": {
    "url": "https://github.com/gouravkhator/stran/issues"
  },
  "author": {
    "name": "Gourav Khator",
    "email": "gouravkhator9@gmail.com"
  },
  "keywords": [
    "strangers",
    "video-calling",
    "video-call",
    "blockchain",
    "peer2peer",
    "p2p",
    "dapp",
    "hardhat",
    "ethereum",
    "smart-contract"
  ]
}